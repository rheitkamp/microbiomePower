Premainder <- function(x) {
rowsize <- dim(x)[1]
colsize <- dim(x)[2]
dims <- dimnames(x)[[2]]
y <- matrix(nrow=rowsize, ncol=colsize)
for(i in 1:rowsize){
total <- apply(X=x,MARGIN=1,sum)
y[i,1] <- x[i,1]/100
total <- total - x[i,1]
for(k in 2:colsize){
y[i,k] <- x[i,k]/ total
total <- total - x[i,k]
}
}
return(matrix(y,nrow=colsize, ncol=rowsize, dimnames=list(dims, "Mean")))
}
